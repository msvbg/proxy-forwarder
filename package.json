{
  "name": "proxy-forwarder",
  "version": "0.1.0",
  "description": "An ES2015 Proxy that forwards every available trap to a new target.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --harmony_proxies",
    "build": "babel index.js > dist/index.js",
    "prepublish": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msvbg/proxy-forwarder.git"
  },
  "keywords": [
    "node",
    "proxy",
    "proxies",
    "forward",
    "forwarder",
    "es2015"
  ],
  "author": "Martin Svanberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msvbg/proxy-forwarder/issues"
  },
  "homepage": "https://github.com/msvbg/proxy-forwarder#readme",
  "dependencies": {
    "harmony-reflect": "1.4.2"
  },
  "devDependencies": {
    "babel": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "expect": "1.13.4",
    "mocha": "2.4.5"
  },
  "ava": {
    "require": "babel-core/register"
  }
}
